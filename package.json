{
  "name": "loopback-ds-iterator-mixin",
  "description": "A mixin to enable easy iteration for a loopback Model.",
  "version": "1.0.1",
  "main": "./lib/index.js",
  "author": "Tom Kirkpatrick @mrfelton",
  "contributors": [
    "Bram Borggreve @beeman"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcube/loopback-ds-iterator-mixin"
  },
  "keywords": [
    "loopback",
    "strongloop",
    "mixin"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullcube/loopback-ds-iterator-mixin/issues"
  },
  "homepage": "https://github.com/fullcube/loopback-ds-iterator-mixin",
  "files": [
    "lib",
    "test"
  ],
  "config": {
    "MOCHA_OPTS": "--recursive test/ --require babel/register --compilers js:babel/register",
    "ISPARTA_OPTS": "--report html",
    "COVERAGE_OPTS": "--statements 100 --functions 100 --branches 100 --lines 100"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./lib --experimental --source-maps-inline -d ./dist",
    "lint": "npm run lint-src && npm run lint-test",
    "lint-src": "eslint lib",
    "lint-test": "eslint test --global describe,it,beforeEach,afterEach",
    "unit": "babel-node -r node_modules/.bin/_mocha $npm_package_config_MOCHA_OPTS",
    "coverage": "babel-node -r node_modules/.bin/isparta cover $npm_package_config_ISPARTA_OPTS node_modules/.bin/_mocha -- $npm_package_config_MOCHA_OPTS",
    "coverage-check": "node node_modules/isparta/node_modules/istanbul/lib/cli check-coverage $npm_package_config_COVERAGE_OPTS",
    "show-report": "node test/open-coverage-report",
    "test": "npm run lint && npm run coverage && (npm run coverage-check || node scripts/open-coverage-report)"
  },
  "dependencies": {
    "async": "^1.4.x",
    "async-iterators": "0.2.2",
    "debug": "2.x",
    "lodash": "^3.9.x"
  },
  "peerDependencies": {
    "loopback-datasource-juggler": ">=2.30.x"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.0.14",
    "bluebird": "latest",
    "chai": "latest",
    "coveralls": "^2.11.4",
    "eslint": "latest",
    "eslint-config-strict": "^6.0.1",
    "isparta": "latest",
    "jscs": "latest",
    "jshint": "latest",
    "loopback": ">=2.22.0",
    "loopback-datasource-juggler": ">=2.30.x",
    "loopback-testing": "1.2.0",
    "mocha": "latest",
    "mocha-sinon": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "sinon-chai": "latest"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
